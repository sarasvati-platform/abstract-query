{
  "name": "Abstract Query",
  "dockerFile": "Dockerfile",
  "workspaceFolder": "/sarasvati/abstract-query",
  "workspaceMount": "source=${localWorkspaceFolder},target=/sarasvati/abstract-query,type=bind,consistency=cached",
  "extensions": [
    "eamodio.gitlens",
    "github.vscode-pull-request-github",
    "orta.vscode-jest",
    "hbenl.vscode-test-explorer",
    "dbaeumer.vscode-eslint",
    "howardzuo.vscode-npm-dependency",
    "eg2.vscode-npm-script",
    "aaron-bond.better-comments",
    "stackbreak.comment-divider",
    "bierner.markdown-mermaid",
    "yzhang.markdown-all-in-one"
  ],
  "postCreateCommand": ".devcontainer/bootstrap.sh"
}